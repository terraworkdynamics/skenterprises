{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production tsc && vite build --mode production",
    "build:dev": "cross-env NODE_ENV=development tsc && vite build --mode development",
    "preview": "vite preview --strictPort --host 127.0.0.1",
    "preview:prod": "vite preview --strictPort --host 0.0.0.0 --port 4173",
    "create-admin": "node --env-file=.env.server scripts/create-admin.mjs",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "deploy:check": "npm run security:audit && npm run build",
    "start": "npm run preview:prod"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.4",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "terser": "^5.44.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-javascript-obfuscator": "^3.1.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.8",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@supabase/supabase-js": "^2.75.0",
    "jspdf": "2.5.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  }
}
